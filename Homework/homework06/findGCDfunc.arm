/* By Michael.
 * Source:
 * https://stackoverflow.com/questions/51189293/how-can-i-call-a-function-written-in-arm-assembly-within-c-code
 */
    .global _findGCD
    .align 2

_findGCD:
    CMP     X1, #0
    BEQ     done

    UDIV    X2, X0, X1
    MSUB    X3, X2, X1, X0

    MOV     X0, X1
    MOV     X1, X3
    
    B       _findGCD

done:
    RET